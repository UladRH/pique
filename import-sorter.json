{
  "wrappingStyle": "prettier",
  "groupRules": [
    [{}],
    [
      "^@pique(.*)shared(.*)",
      "^@pique(.*).module",
      "^@pique(.*).service",
      "^@pique(.*).controller",
      "^@pique(.*).entity",
      "^@pique(.*)dto",
      "^@pique"
    ],
    [
      "^[./](.*)shared(.*)",
      "^[./](.*).module",
      "^[./](.*).service",
      "^[./](.*).controller",
      "^[./](.*).entity",
      "^[./](.*)dto",
      "^[./]"
    ]
  ]
}
