{
  "name": "@pique/backend",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Uladzislau Hlebovich",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "format-imports src/ && prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrate:create": "ts-node ../node_modules/typeorm/cli.js migrations:create",
    "migrate:generate": "ts-node ../node_modules/typeorm/cli.js migrations:generate",
    "migrate:run": "ts-node ../node_modules/typeorm/cli.js migration:run",
    "seed": "ts-node ../node_modules/typeorm-seeding/dist/cli.js seed",
    "seed:dummy": "ts-node ../node_modules/typeorm-seeding/dist/cli.js seed --seed DummySeed"
  },
  "dependencies": {
    "@codebrew/nestjs-storage": "^0.1.4",
    "@mikro-orm/cli": "^4.5.7",
    "@mikro-orm/core": "^4.5.7",
    "@mikro-orm/nestjs": "^4.2.0",
    "@mikro-orm/seeder": "^5.0.0-dev.112",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@mikro-orm/sqlite": "^4.5.7",
    "@nestjs/common": "^7.6.17",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.17",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^7.6.17",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/typeorm": "^7.1.5",
    "@slynova/flydrive": "^1.0.3",
    "argon2": "^0.28.1",
    "better-sqlite3": "^7.4.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cookie-session": "^1.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "sharp": "^0.28.3",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34",
    "typeorm-naming-strategies": "^2.0.0",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.1",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.17",
    "@types/cookie-session": "^2.0.42",
    "@types/express": "^4.17.12",
    "@types/faker": "^5.5.6",
    "@types/jest": "^26.0.23",
    "@types/multer": "^1.4.5",
    "@types/node": "^14.17.2",
    "@types/sharp": "^0.28.3",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "format-imports": "^2.4.0",
    "jest": "^27.0.4",
    "prettier": "^2.3.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.2",
    "ts-loader": "^9.2.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  }
}
